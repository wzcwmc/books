<!--            
              CONTENT 
                        -->
<div id="content" class="white">
	<h1><img src="img/icons/posts.png" alt="" /> 管理员新增图书</h1>
	<div class="bloc">
		<div class="title">请输入新增图书</div>
		<div class="content">
			<div class="input">
				<label for="input1">Text input</label>
				<input type="text" id="input1" />
				Some informations on how to use this field </div>
			<div class="input medium error">
				<label for="input2">Medium input with error</label>
				<input type="text" id="input2" />
				<span class="error-message">This field can't be empty !</span> </div>
			<div class="input long">
				<label for="input3">请写入新增图书信息</label>
				<input type="text" id="input3" />
			</div>
			<div class="input">
				<label for="file">请选择图书封面</label>
				<input type="file" id="file" />
			</div>
			<div class="input">
				<label class="label">核实</label>
				<input type="checkbox" id="check1" checked="checked"/>
				<label for="check1" class="inline">是否核实图书信息</label>
				<br/>
				<input type="checkbox" id="check2" />
				<label for="check2" class="inline">再次核实</label>
				<br/>
			</div>
			<div class="input">
				<label class="label">嗯哼</label>
				<input type="radio" id="radio1" name="radiobutton"  checked="checked"/>
				<label for="radio1" class="inline">嘟嘟</label>
				<br/>
				<input type="radio" id="radio2"  name="radiobutton"/>
				<label for="radio2" class="inline">ooxx</label>
			</div>
			<div class="input">
				<label for="select">管理员名单</label>
				<select name="select" id="select">
					<option value="1">刘晓堃</option>
					<option value="2">王超</option>
					<option value="3">庄鹏成</option>
				</select>
				请认真核实信息 </div>
			<div class="input textarea">
				<label for="textarea1">图书管理日志</label>
				<textarea name="text" id="textarea1" rows="7" cols="4"></textarea>
			</div>
			<div class="input">
				<label for="input4">算是日期吧</label>
				<input type="text" class="datepicker" id="input4"/>
			</div>
			<div class="input textarea">
				<label for="textarea2">写点什么好呢 (<a href="https://github.com/akzhan/jwysiwyg">跳转日志页面</a>)</label>
				<textarea name="text" id="textarea2" rows="7" class="wysiwyg" cols="4">
                Here you <em>can have</em> some <strong>HTML Content</strong>
            </textarea>
			</div>
			<div class="input">
				<label>赏金 : $<span></span></label>
				<input type="text" class="range min-10 max-60" value="35" />
			</div>
			<div class="input">
				<label for="iphonecheck" class="label">是否与厂商电话核实</label>
				<input type="checkbox" id="iphonecheck" class="iphone"/>
			</div>
			<div class="submit">
				<input type="submit" value="不知道是啥" />
				<input type="reset" value="只是个按钮" class="black"/>
				<input type="reset" value="只是个按钮" class="white"/>
			</div>
		</div>
	</div>
